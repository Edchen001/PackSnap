<div class="container">
	<div class="dash-header">
		<div class="weather">
			<h1 class="destination-title"><%= @location.address %></h1>
			<h2 class="temp"><%= @weather %>&deg;F</h1>
			<h3 class="summary"><%= @summary %></h3>
		</div>
	</div>
<div id="mi-slider" class="mi-slider">
	<ul>
	<% @suggested_items.each do |item| %>
		<li><%= image_tag item.image.url(:thumb) %><h4><%= item.name %></li>
		<% end %>
	</ul>
	<nav>
		<a href="#">PackSnap Suggestions</a>
	</nav>
</div>
	<div id="grid-gallery" class="grid-gallery">
		<%= render partial: "dash/grip_wrap" %>
		<%= render partial: "dash/slideshow" %>
	</div>
</div>
	<%= render partial: "dash/hidden_form" %>
