<div class="dash">
	<div class="weather_widget">
		<iframe id="forecast_embed" type="text/html" frameborder="0" height="245" width="60%" src="http://forecast.io/embed/#lat=&lon=&"> </iframe>
	</div>

	<% users.each do |user| %>
	 <%= user %>
	<% end %>
  <% items.each do |item| %>
    <%= item.name %>
    <%= image_tag item.photo.image(:thumb) %>
  <% end %>
  <% if current_user %>
	 <%= link_to "Upload Your Pack!", new_photo_path, id: "new-photo-link", class: "pure-button new-trip-link" %>
  <% end %>
</div>